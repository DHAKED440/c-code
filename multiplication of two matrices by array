#include<stdio.h>
int main () {
      int M1[100][100], M2[100][100], M3[100][100],r1,c1,r2,c2;
      printf("Enter the value of r1 and c1 for matrix1 : ");
      scanf("%d %d", &r1,&c1);
       printf("Enter the value of r2 and c2 for matrix2 : ");
      scanf("%d %d", &r2,&c2);
      if(c1 != r2){
        printf("Matrix1 and Matrix2 can not be multiply");
        return 0;
      }

       M1[r1][c1];
      printf("all the elements of matrix1 : ");
    for (int i=0; i<r1; i++){
        for(int j=0; j<c1; j++ ){
            scanf("%d", &M1[i][j]);
        }
    }
 
    printf("matrix1 :\n");
     for(int i=0; i<r1; i++){
        for (int j=0; j<c1; j++ ) {
            printf("%d ", M1[i][j]);
        }
        printf(" \n");
    }

     M2[r2][c2];
      printf("all the elements of matrix2 : ");
    for (int i=0; i<r2; i++){
        for(int j=0; j<c2; j++ ){
            scanf("%d", &M2[i][j]);
        }
    }
 
    printf("matrix2 :\n");
     for(int i=0; i<r2; i++){
        for (int j=0; j<c2; j++ ) {
            printf("%d ", M2[i][j]);
        }
        printf(" \n");
    }

    for(int i=0; i<r1; i++){
        for(int j=0; j<c2; j++){
            int sum=0;
            for(int k=0; k<r2; k++){
                sum+=M1[i][k]*M2[k][j];
            }
            M3[i][j]=sum;
        }
    }

    printf("multiplication of matrix1 and matrix2 : \n");
    for(int i=0; i<r1; i++){
        for(int j=0; j<c2; j++){
            printf("%d",M3[i][j] );
        }
        printf(" \n");
    }
    return 0;
    }
