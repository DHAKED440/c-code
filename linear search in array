#include<stdio.h>
#include<limits.h>

int linearsearch(int arr[], int size , int key){
    for(int i=0; i<size ; i++){
        if(arr[i] == key ){
            return 1;
        }
    }
    return 0;
}
int main () {
    int size=5;
int arr[]={12 , 34, 56, -23 , 34};
int key;
printf("Enter the search element : ");
scanf("%d", &key);
int found = linearsearch(arr,size,key);
if(found == 1 ) {
    printf("key is present ");
} else {
    printf("Key is not present");
}
return 0;
}
