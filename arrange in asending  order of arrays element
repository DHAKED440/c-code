#include<stdio.h>
int main() {
    int size;
    printf("ENter the size of array : ");
       
       scanf("%d", &size);
       int arr[size];
       printf("all the elements of array : ");
       for(int i=0; i<size; i++ ){
        scanf("%d", &arr[i]);
       }

       for(int i=0; i<size; i++){
        for(int j=0; j<size; j++){
            if(arr[i]<arr[j]){
                int temp=arr[j];
                arr[j]=arr[i];
                arr[i]=temp;
            }
        }
       }   
       printf("array in asending order : ");
       for(int i=0; i<size; i++){
        printf("%d ", arr[i]);
       }
return 0;
}
