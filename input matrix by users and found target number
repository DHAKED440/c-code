#include<stdio.h>
#include<limits.h>

int search(int arr[3][4], int target , int i, int j){
    for(int i=0; i<3; i++){
    for(int j=0; j<4; j++){
        if(arr[i][j]==target){
            return 1;
        }
    }
   }
   return 0;
}
int main() {
   int arr[3][4];
//    for taking input
   for(int i=0; i<3; i++){
    for(int j=0; j<4; j++){
        scanf("%d", &arr[i][j]);
    }
   }
// for output
for(int i=0; i<3; i++){
    for(int j=0; j<4; j++){
        printf("%d ", arr[i][j]);
    }
    printf("\n");
   }
   
int target;
   printf("Enter the target : ");
   scanf("%d", &target);
int found=search(arr,target,3,4);
   if( found == 1 ){
    printf("Target is present");
   } else {
    printf("Target is not present");
   }
    return 0;
}
