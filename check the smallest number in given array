#include<stdio.h>
#include<limits.h>

int main () {
     int size=5;
     int num[size];
      int smallest=INT_MAX;
     printf("values in array : ");
     for(int i=0; i<size; i++ ){
        scanf("%d", &num[i]);
        if(num[i] < smallest){
            smallest=num[i];
        }
     }
     
   printf("smallest value = %d",smallest);
     return 0;
    }
