#include<stdio.h>
int main () {
  int n,d,a=0;
  printf("Enter the value of n : ");
    scanf("%d", &n);
    printf("Enter the  common difference : ");
    scanf("%d", &d);
    for(int i=0; i<=(n/d); i++ ){
        int ans=0;
        ans+= (a+i*d);
        printf(" %d + ",ans);
    }
    return 0;
  }
