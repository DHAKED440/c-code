#include<stdio.h>
int main () {
  int n,d,a;
  printf("Enter the value of n : ");
    scanf("%d", &n);
    printf("Enter the  common difference : ");
    scanf("%d", &d);
    printf("Enter the first term : ");
    scanf("%d", &a);
    for(int i=a; i<=n; i+=d ){
        printf(" %d + ",i);
    }
    return 0;
  }
