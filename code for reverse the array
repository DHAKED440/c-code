#include<stdio.h>
#include<limits.h>

int main() {
    int size=5;
    int arr[size];
    printf("Enter the elements in array : ");
    for (int i=0; i<size; i++){
      scanf("%d", &arr[i]);
    }
    int start=0,end=(size-1);
 while(start < end){
    int temp=arr[start];
    arr[start]=arr[end];
    arr[end]=temp;
    start++;
    end--;
 }
//  printf("After swap array = { %d %d %d %d %d }",arr[0],arr[1],arr[2],arr[3],arr[4]);
printf("REverse of array = ");
for (int i=0; i<size; i++){
    printf("%d ",arr[i]);
}
return 0;
}
