#include<stdio.h>
#include<limits.h>

int main () {
     int size=5;
     int num[size];
      int max=INT_MIN;
     printf("values in array : ");
     for(int i=0; i<size; i++ ){
        scanf("%d", &num[i]);
        if(num[i] > max){
            max=num[i];
        }
     }
     
   printf("largest value = %d",max);
     return 0;
    }
