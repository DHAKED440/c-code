#include<stdio.h>

int main () {
    int n,result=0;
    printf("Enter the number : ");
    scanf("%d", &n);

    int m=n;
 while( m>0 ){
    int fac=1;
    int rem=m%10;
    for(int i=1; i<=rem; i++){
          fac*=i;
    }
    result += fac;
    m=m/10;
 }

 if(result==n){
    printf("%d is an strong number",n);
 } else {
    printf("%d is not an strong number ",n );
 }
 return 0;
}
